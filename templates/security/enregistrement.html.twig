{% extends "base.html.twig" %}

{% block body %}
<h1>Inscription nouveau Membre</h1>
        {{form_start(form)}}
        {{form_row(form.nom,{'attr':{'placeholder':'Votre Nom.....'}}) }}
        {{form_row(form.prenom,{'attr':{'placeholder':'Votre Prenom.....'}}) }}
        {{form_row(form.email,{'attr':{'placeholder':'Votre Adresse Mail.....'}}) }}
        {{form_row(form.passeword,{'attr':{'placeholder':'Votre Mot De Passe.....'}}) }}
        {{form_row(form.confirm_password,{'attr':{'placeholder':' Confirmer Votre Mot de Passe.....'}}) }}

        <button type="submit" class="btn btn-success">Inscription !</button>
        {{form_end(form)}} 


{% endblock %}